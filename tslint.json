{
  "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules",
      "coverage/lcov-report/*.js",
      "tslint-config-prettier"
    ]
  },
  "defaultSeverity": "warning",
  "rules": {
    "interface-name": false,
    "jsx-no-lambda": false
  }
}
